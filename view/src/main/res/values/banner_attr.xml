<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Banner">
        <!--   是否重复播放     -->
        <attr name="auto_play" format="boolean"/>
        <!--  播放时间间隔 以毫秒为单位       -->
        <attr name="interval" format="integer"/>

        <attr name="indicator_normal_color" format="color"/>
c
        <attr name="indicator_choose_color" format="color"/>
        
        <attr name="indicator_type">
            <enum name="circle" value="0"/>
            <enum name="square" value="1"/>
        </attr>

        <attr name="spacing" format="dimension"/>

        <attr name="indicator_size" format="dimension"/>

    </declare-styleable>

</resources>